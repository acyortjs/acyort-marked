{
  "name": "@acyort/markdown",
  "version": "1.0.0",
  "description": "Markdown parser with code highlight",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:cover",
    "test:lint": "eslint .",
    "test:unit": "mocha --reporter dot --require intelli-espower-loader ./test",
    "test:cover": "nyc npm run test:unit",
    "coverage": "nyc report --reporter=lcov npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acyortjs/markdown.git"
  },
  "keywords": [
    "markdown",
    "parser",
    "code",
    "highlight"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyortjs/markdown/issues"
  },
  "homepage": "https://github.com/acyortjs/markdown#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "power-assert": "^1.6.1"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "marked": "^0.5.0"
  }
}
